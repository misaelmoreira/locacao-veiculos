<form action="/administrador/logar" method="post">
  <div class="form-group">
    <label for="exampleInputEmail1">Email </label>
    <input type="email" class="form-control" id="email" name="login" aria-describedby="emailHelp" placeholder="Digite Email">
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Senha</label>
    <input type="password" class="form-control" name="senha" id="exampleInputPassword1" placeholder="Senha">
  </div>
  <div class="form-check">
    <input type="checkbox" class="form-check-input" id="lembrar">
    <label class="form-check-label" for="exampleCheck1" name="lembrar">Check me out</label>
  </div>

  <button type="submit" class="btn btn-primary">Logar</button>
</form>